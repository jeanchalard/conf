xkb_keymap {
        // xkb_XXX { include "YYY(ZZZ)" } means to look up in /usr/share/X11/xkb/XXX/YYY for a section named ZZZ
	xkb_keycodes  { include "evdev+aliases(qwerty)" };
	xkb_types     { include "complete" };
	xkb_compat    { include "complete" };
	xkb_symbols   { include "pc+anterak" };
	xkb_geometry  { include "pc(pc105)" }; // Used to be pc(jp106), but now there seems to be no non-vendor-specific JP geometry ? Should be jp109 really for a modern keyboard
};
