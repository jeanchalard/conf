default alphanumeric_keys modifier_keys
xkb_symbols "basic" {
  include "anterak(alpha)"
  include "anterak(special)"
  include "anterak(modifiers)"
  include "anterak(editing)"
  include "anterak(inet)"
  include "anterak(other)"
};

partial hidden alphanumeric_keys
xkb_symbols "alpha" {
    key <AE01> { [ 1,	             exclam,        onesuperior,      onehalf        ] };
    key <AE02> { [ 2,	             at,            twosuperior,      twothirds      ] };
    key <AE03> { [ 3,	             numbersign,    threesuperior,    onethird       ] };
    key <AE04> { [ 4,	             dollar,        foursuperior,     onequarter     ] };
    key <AE05> { [ 5,	             percent,       fivesuperior,     threequarters  ] };
    key <AE06> { [ 6,	             asciicircum,   sixsuperior,      U2200          ] }; // For all
    key <AE07> { [ 7,                ampersand,     sevensuperior,    U2203          ] }; // There exists
    key <AE08> { [ 8,	             asterisk,      eightsuperior,    U2204          ] }; // There does not exist
    key <AE09> { [ 9,	             parenleft,     ninesuperior,     U2208          ] }; // Element of
    key <AE10> { [ 0,	             parenright,    zerosuperior,     infinity       ] };
    key <AE11> { [ bracketleft,	     braceleft,     dead_abovedot,    U0307          ] };
    key <AE12> { [ bracketright,     braceright,    dead_belowdot,    U0323          ] };
    key <AE13> { [ BackSpace,        BackSpace,     U0336,            U0336          ] }; // Combining strikethrough
    key <BKSP> { [ BackSpace,        BackSpace,     U0336,            U0336          ] }; // Combining strikethrough

    key <AD01> { [ apostrophe,       quotedbl,      dead_acute,       U0301          ] };
    key <AD02> { [ comma,            less,          dead_grave,       U0300          ] };
    key <AD03> { [ period,	     greater,       dead_macron,      U0304          ] };
    key <AD04> { [ p,                P,             dead_breve,       U0306          ] };
    key <AD05> { [ y,                Y,             dead_circumflex,  U0302          ] };
    key <AD06> { [ f,                F,             dead_diaeresis,   U0308          ] };
    key <AD07> { [ g,                G,             ssharp,           EuroSign       ] };
    key <AD08> { [ c,                C,             dead_cedilla,     U0327          ] };
    key <AD09> { [ r,                R,             dead_ogonek,      U0328          ] };
    key <AD10> { [ l,                L,             dead_abovering,   U030A          ] };
    key <AD11> { [ slash,            question,      dead_doubleacute, U030B          ] };
    key <AD12> { [ equal,            plus,          dead_tilde,       U0303          ] };

    key <AC01> { [ a,                A,             agrave,           Agrave         ] };
    key <AC02> { [ o,	             O,             ugrave,           Ugrave         ] };
    key <AC03> { [ e,	             E,             eacute,           Eacute         ] };
    key <AC04> { [ u,	             U,             egrave,           Egrave         ] };
    key <AC05> { [ i,	             I,             ccedilla,         Ccedilla       ] };
    key <AC06> { [ d,	             D,             adiaeresis,       Adiaeresis     ] };
    key <AC07> { [ h,	             H,             odiaeresis,       Odiaeresis     ] };
    key <AC08> { [ t,	             T,             ediaeresis,       Ediaeresis     ] };
    key <AC09> { [ n,	             N,             udiaeresis,       Udiaeresis     ] };
    key <AC10> { [ s,	             S,             idiaeresis,       Idiaeresis     ] };
    key <AC11> { [ minus,            underscore,    oe,               OE             ] };
    key <BKSL> { [ backslash,        bar,           questiondown,     exclamdown     ] };

    key <AB01> { [ semicolon,	     colon,         acircumflex,      Acircumflex    ] };
    key <AB02> { [ q,	             Q,             ocircumflex,      Ocircumflex    ] };
    key <AB03> { [ j,	             J,             ecircumflex,      Ecircumflex    ] };
    key <AB04> { [ k,	             K,             ucircumflex,      Ucircumflex    ] };
    key <AB05> { [ x,	             X,             icircumflex,      Icircumflex    ] };
    key <AB06> { [ b,	             B,             uparrow,          copyright    ] };
    key <AB07> { [ m,	             M,             downarrow,        mu    ] };
    key <AB08> { [ w,	             W,             leftarrow,        guillemotleft  ] };
    key <AB09> { [ v,	             V,             rightarrow,       guillemotright ] };
    key <AB10> { [ z,	             Z,             ae,               AE             ] };
    key <AB11> { [ question,         exclam,        questiondown,     exclamdown     ] };

    // TODO : what to do with this ?
    key <LSGT> { [ less,             greater,       bar,              brokenbar ] };
};

partial hidden alphanumeric_keys
xkb_symbols "special" {
    key <ESC>  { [ Escape            ] };

    // TODO : define something for AltGr+TLDE
    key <TLDE> { [ grave,            asciitilde       ] };
    // TODO : have some real use for AltGr+Tab
    key <TAB>  { [ Tab,              ISO_Left_Tab     ] };

// Turn Caps into another AltGr
//    key <CAPS> { [ ISO_Level3_Latch, ISO_Level3_Latch ] };

    // U3000 = double width space ; U2588 = full block
    key <SPCE> { [ space,            space,      nobreakspace,     U2588          ] };
    key <RTRN> { [ Return            ]  };
};

partial hidden modifier_keys
xkb_symbols "modifiers" {

    key <NMLK> { [ Num_Lock         ] };
    key <LFSH> { [ Shift_L          ] };
    key <LCTL> { [ Control_L        ] };
    key <LWIN> { [ Super_L          ] };

    key <RTSH> { [ Shift_R          ] };
    key <RCTL> { [ Control_R        ] };
    key <RALT> { [ ISO_Level3_Latch, ISO_Level3_Latch ] };
    key <RWIN> { [ Super_R          ] };
    key <MENU> { [ Menu             ] };

    // Beginning of modifier mappings.
    modifier_map Shift   { Shift_L, Shift_R };
    modifier_map Lock    { Caps_Lock };
    modifier_map Control { Control_L, Control_R };
    modifier_map Mod2    { Num_Lock };
    modifier_map Mod4    { Super_L, Super_R };

    // Fake keys for virtual<->real modifiers mapping:
    key <LVL3> { [ ISO_Level3_Shift ] };
    key <MDSW> { [ Mode_switch      ] };
    modifier_map Mod5 { <LVL3>, <MDSW> };

    key <ALT>  { [ NoSymbol, Alt_L ] };

    key <META> { [ NoSymbol, Meta_L ] };
    modifier_map Mod1   { <META> };

    key <SUPR> { [ NoSymbol, Super_L ] };
    modifier_map Mod4   { <SUPR> };

    key <HYPR> { [ NoSymbol, Hyper_L ] };
    modifier_map Mod4   { <HYPR> };
};

