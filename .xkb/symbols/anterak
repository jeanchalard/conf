xkb_symbols "editing" {
    key <PRSC> {
      type= "PC_ALT_LEVEL2",
      symbols[Group1]= [ Print, Sys_Req ]
    };
    key <SCLK> { [ Scroll_Lock ] };
    key <PAUS> {
      type= "PC_CONTROL_LEVEL2",
      symbols[Group1]= [ Pause, Break ]
    };
    key  <INS> { [ Insert ] };
    key <HOME> { [ Home ] };
    key <PGUP> { [ Prior ] };
    key <DELE> { [ Delete ] };
    key  <END> { [ End ] };
    key <PGDN> { [ Next ] };

    key   <UP> { [ Up ] };
    key <LEFT> { [ Left ] };
    key <DOWN> { [ Down ] };
    key <RGHT> { [ Right ] };
};

